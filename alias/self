# 本地打开浏览器调试，后续alias接入cli
# domain: localhost
alias l3000="chrome http://localhost:3000"
alias l3="chrome http://localhost:3000"

alias l3001="chrome http://localhost:3001"
alias l301="chrome http://localhost:3001"

alias l4="chrome http://localhost:4000"
alias l4000="chrome http://localhost:4000"

alias l8000="chrome http://localhost:8000"
alias l8="chrome http://localhost:8000"

alias l801="chrome http://localhost:8001"
alias l8001="chrome http://localhost:8001"

alias l8080="chrome http://localhost:8080"
alias l88="chrome http://localhost:8080"
alias l808="chrome http://localhost:8080"

alias l9000="chrome http://localhost:9000"
